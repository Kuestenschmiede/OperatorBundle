{
  "name": "gutesio/operator",
  "type": "contao-bundle",
  "description": "The bundle for the gutes.io operator instances.",
  "keywords": ["gutes.io"],
  "homepage": "https://gutes.io",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Kuestenschmiede GmbH Software & Design",
      "homepage": "https://kuestenschmiede.de",
      "role": "Operator"
    },
    {
      "name": "con4gis community",
      "homepage": "https://con4gis.org",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@con4gis.org",
    "issues": "https://github.com/Kuestenschmiede/OperatorBundle/issues",
    "source": "https://github.com/Kuestenschmiede/OperatorBundle"
  },
  "require": {
    "php": "^7.1",
    "contao/core-bundle": "^4.9",
    "con4gis/core": "^4.1",
    "con4gis/maps": "^7.3",
    "gutesio/data-model": "^1.0"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "gutesio\\OperatorBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "gutesio\\OperatorBundle\\ContaoManager\\Plugin"
  }
}
